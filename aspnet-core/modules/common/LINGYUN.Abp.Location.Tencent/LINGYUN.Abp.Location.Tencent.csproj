    <PackageReference Include="Volo.Abp.Json" Version="3.1.0" />
    <PackageReference Include="Volo.Abp.Threading" Version="3.1.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\LINGYUN.Abp.Location\LINGYUN.Abp.Location.csproj" />
